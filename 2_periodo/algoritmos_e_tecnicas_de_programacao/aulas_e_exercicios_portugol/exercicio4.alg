algoritmo "semnome"
// Função :
// Autor :
// Data : 05/04/2022
// Seção de Declarações 
var
temperaturaCelsius: real
temperaturaFahrenheit: real
inicio
Escreva("Digite a temperatura em Celsius: ")
Leia(temperaturaCelsius)

temperaturaFahrenheit <- ((9 * temperaturaCelsius) + 160) / 5
Escreva(temperaturaCelsius, "°C em Fahrenheit são ", temperaturaFahrenheit, "°F.")
fimalgoritmo
